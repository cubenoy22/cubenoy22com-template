{
  "compilerOptions": {
    "target": "ES2019",
    "module": "CommonJS",
    "jsx": "preserve",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["esnext"],
    "noEmit": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
  },
}
